{
  "name": "sergix44/laraver",
  "description": "API endpoint versioning made easy",
  "license": "MIT",
  "authors": [
    {
      "name": "Sergio Brighenti",
      "email": "sergio@brighenti.me",
      "homepage": "http://sergio.brighenti.me"
    }
  ],
  "homepage": "https://github.com/sergix44/laraver",
  "keywords": [
    "Laravel",
    "LaraVer"
  ],
  "require": {
    "php": "^7.2",
    "illuminate/support": "~6|~7|~8",
    "illuminate/routing": "~6|~7|~8"
  },
  "require-dev": {
    "phpunit/phpunit": "~9.0 | ~8.0",
    "orchestra/testbench": "~5|~6"
  },
  "autoload": {
    "psr-4": {
      "SergiX44\\LaraVer\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SergiX44\\LaraVer\\Tests\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "SergiX44\\LaraVer\\LaraVerServiceProvider"
      ]
    }
  }
}
